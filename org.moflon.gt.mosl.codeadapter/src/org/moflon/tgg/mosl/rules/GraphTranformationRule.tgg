#using org.moflon.gt.mosl.codeadapter.*
#using AttrCondDefLibrary.*

#rule GraphTranformationRule #with org.moflon.gt.mosl.codeadapter

#source { 
	gtf : GraphTransformationFile
}

#target {
	trgEPackage : EPackage
}

#correspondence {
	++ graphToPackage : GraphTransformationToEPackage{
		#src->gtf
		#trg->trgEPackage
	}
}

#attributeConditions {
	eq(gtf.name, trgEPackage.name)
}
